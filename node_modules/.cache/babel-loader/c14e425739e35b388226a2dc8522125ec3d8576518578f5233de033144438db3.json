await loading.present();\r\n\r\n                await checkLocationPermission();\r\n              \r\n                if(permission.value == 'denied'){\r\n                  //show alert\r\n                  // const alert = await alertController.create({\r\n                  //   header: 'Location Permission Denied',\r\n                  //   message: 'Please enable location permission to use this feature.',\r\n                  //   buttons: ['OK']\r\n                  // });\r\n                  // await alert.present();\r\n                \r\n\r\n                }else{\r\n                  await axios.post('https://gae.co.bw/api/v1/report/panic_mode', form)\r\n                  // await axios.post('http://127.0.0.1:8000/api/v1/auth/register', form)\r\n                  .then((response) => {\r\n                      console.log(response.data);\r\n                      form.location = '';\r\n                      success.value = true\r\n                      loading.dismiss();\r\n\r\n                  })\r\n                  .catch((err) => {\r\n                      error.value = err.response.data.error\r\n                      console.log(err.response.data);\r\n                      loading.dismiss();\r\n                  });\r\n                }\r\n\r\n                //dismiss loading\r\n                loading.dismiss();\r\n              }\r\n\r\n              // Check for location permission on mount\r\n              const checkLocationPermission = async () => {\r\n                const status = await Geolocation.checkPermissions();\r\n\r\n                permission.value = status;\r\n\r\n                if (status.location === 'denied') {\r\n                  console.log('Location permission denied.');\r\n                } else if (status.location === 'granted') {\r\n                  console.log('Location permission granted.');\r\n                  getCurrentLocation();\r\n                } else {\r\n                  console.log('Location permission not re