cation.value = pos;\n    };\n    const successCallback = async position => {\n      const latitude = position.coords.latitude;\n      const longitude = position.coords.longitude;\n      location.value = position;\n      console.log(\"Latitude: \" + latitude + \" Longitude: \" + longitude);\n    };\n    const errorCallback = error => {\n      console.log(\"Unable to retrieve your location due to \" + error.code + \": \" + error.message);\n    };\n    return {\n      panic,\n      reports,\n      locationIcon\n    };\n  },\n  methods: {\n    // handleFilePondInit: function () {\n    // console.log(\"FilePond has initialized\");\n\n    // // FilePond instance methods are available on `this.$refs.pond`\n    // },\n  },\n  components: {\n    // FilePond,\n    IonButtons,\n    IonContent,\n    IonHeader,\n    IonMenuButton,\n    IonPage,\n    IonTitle,\n    IonToolbar,\n    IonCard,\n    IonCardHeader,\n    IonCardContent,\n    IonItem,\n    IonLabel,\n    IonList,\n    IonIcon\n  }\n};","map":{"version":3,"names":["IonButtons","IonContent","IonHeader","IonMenuButton","IonPage","IonTitle","IonToolbar","IonItem","IonLabel","IonList","IonIcon","IonCard","IonCardHeader","loadingController","alertController","IonCardContent","Geolocation","GeolocationPosition","PermissionStatus","location","locationIcon","ref","reactive","onMounted","useAuthStore","axios","router","Preferences","name","data","myFiles","setup","authStore","permission","error","success","reports","alert","form","value","loading","create","message","spinner","present","navigator","geolocation","getCurrentPosition","successCallback","errorCallback","console","log","get","headers","Authorization","token","then","response","dismiss","catch","err","header","buttons","panic","x","array","coords","latitude","longitude","post","checkLocationPermission","status","checkPermissions","getCurrentLocation","requestLocationPermission","requestPermissions","pos","position","code","methods","components"],"sources":["C:\\Users\\